  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m4rc0d3r/js-std.git"
  },
  "packageManager": "pnpm@10.11.0",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
